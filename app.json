{
	"expo": {
		"name": "iflab-mobile",
		"slug": "iflab-mobile",
		"scheme": "iflab-mobile",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icons/icon-512-maskable.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/icons/icon-512-maskable.png",
			"resizeMode": "contain",
			"backgroundColor": "#26580B"
		},
		"ios": {
			"supportsTablet": true,
			"icon": "./assets/icons/icon-512-maskable.png"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icons/icon-512-maskable.png",
				"backgroundColor": "#26580B"
			},
			"icon": "./assets/icons/icon-512-maskable.png",
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false
		},
		"web": {
			"favicon": "./assets/icons/icon-512-maskable.png"
		},
		"plugins": ["expo-router"]
	}
}
